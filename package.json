{
  "name": "self-care-guide-for-wellness",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "overrides": {
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "vitest": "^2.0.5"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "cross-env NODE_OPTIONS=--no-warnings vitest",
    "test:run": "cross-env NODE_OPTIONS=--no-warnings vitest run --no-file-parallelism",
    "test:coverage": "cross-env NODE_OPTIONS=--no-warnings vitest run --coverage --no-file-parallelism",
    "test:ui": "cross-env NODE_OPTIONS=--no-warnings vitest --ui",
    "test:watch": "cross-env NODE_OPTIONS=--no-warnings vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@google/genai": "^1.11.0",
    "@vercel/node": "^3.0.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "msw": "^2.1.7",
    "postcss": "^8.4.32",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^2.0.5"
  }
}
